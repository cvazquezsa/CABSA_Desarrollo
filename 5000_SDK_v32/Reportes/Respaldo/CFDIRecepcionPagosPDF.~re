[Vista.ListaCampos]
(Inicio)=Estacion
Estacion=ID
ID=ModuloID
ModuloID=Modulo
Modulo=CadenaOriginal
CadenaOriginal=SelloSAT
SelloSAT=RFCEmisor
RFCEmisor=Emisor
Emisor=RFCReceptor
RFCReceptor=Receptor
Receptor=FechaPago
FechaPago=FormaPago
FormaPago=NumOperacion
NumOperacion=RFCBancoEmisor
RFCBancoEmisor=RFCBancoOrdenante
RFCBancoOrdenante=BancoOrdenante
BancoOrdenante=CuentaOrdenante
CuentaOrdenante=CuentaBeneficiaria
CuentaBeneficiaria=MontoPago
MontoPago=MonedaPago
MonedaPago=TipoCambioPago
TipoCambioPago=SelloEmisor
SelloEmisor=IDDR
IDDR=SerieDR
SerieDR=FolioDR
FolioDR=MonedaDR
MonedaDR=TipoCambioDR
TipoCambioDR=MetodoPagoDR
MetodoPagoDR=Parcialidad
Parcialidad=ImportePagado
ImportePagado=SaldoAnterior
SaldoAnterior=SaldoInsoluto
SaldoInsoluto=(Fin)

[Vista.SQL]
(Inicio)=SELECT Estacion,
SELECT Estacion,=ID,
ID,=ModuloID,
ModuloID,=Modulo,
Modulo,=RFCEmisor,
RFCEmisor,=Emisor,
Emisor,=RFCReceptor,
RFCReceptor,=Receptor,
Receptor,=FechaPago,
FechaPago,=FormaPago,
FormaPago,=NumOperacion,
NumOperacion,=RFCBancoEmisor,
RFCBancoEmisor,=RFCBancoOrdenante,
RFCBancoOrdenante,=BancoOrdenante,
BancoOrdenante,=CuentaOrdenante,
CuentaOrdenante,=CuentaBeneficiaria,
CuentaBeneficiaria,=MontoPago,
MontoPago,=MonedaPago,
MonedaPago,=TipoCambioPago,
TipoCambioPago,=Cadenaoriginal,
Cadenaoriginal,=SelloEmisor,
SelloEmisor,=SelloSAT,
SelloSAT,=IDDR,
IDDR,=SerieDR,
SerieDR,=FolioDR,
FolioDR,=MonedaDR,
MonedaDR,=TipoCambioDR,
TipoCambioDR,=MetodoPagoDR,
MetodoPagoDR,=Parcialidad,
Parcialidad,=ImportePagado,
ImportePagado,=SaldoAnterior,
SaldoAnterior,=SaldoInsoluto
SaldoInsoluto=FROM CFDICobroParcialDatosPDF
FROM CFDICobroParcialDatosPDF=WHERE Estacion <IGUAL> '{EstacionTrabajo}' AND ID <IGUAL> '{Info.CantidadPendiente}'
WHERE Estacion <IGUAL> '{EstacionTrabajo}' AND ID <IGUAL> '{Info.CantidadPendiente}'=(Fin)










