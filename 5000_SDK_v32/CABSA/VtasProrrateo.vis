[Vista.SQL]
(Inicio)=SELECT
SELECT=VtasProrrateo.ID,
VtasProrrateo.ID,=VtasProrrateo.RenglonID,
VtasProrrateo.RenglonID,=VtasProrrateo.Articulo,
VtasProrrateo.Articulo,=VtasProrrateo.EmpresaProrrateo,
VtasProrrateo.EmpresaProrrateo,=VtasProrrateo.SucursalProrrateo,
VtasProrrateo.SucursalProrrateo,=VtasProrrateo.CCProrrateo,
VtasProrrateo.CCProrrateo,=VtasProrrateo.CC2Prorrateo,
VtasProrrateo.CC2Prorrateo,=VtasProrrateo.CC3Prorrateo,
VtasProrrateo.CC3Prorrateo,=VtasProrrateo.EspacioProrrateo,
VtasProrrateo.EspacioProrrateo,=VtasProrrateo.VINProrrateo,
VtasProrrateo.VINProrrateo,=VtasProrrateo.ProyectoProrrateo,
VtasProrrateo.ProyectoProrrateo,=VtasProrrateo.UENProrrateo,
VtasProrrateo.UENProrrateo,=VtasProrrateo.ActividadProrrateo,
VtasProrrateo.ActividadProrrateo,=VtasProrrateo.Importe,
VtasProrrateo.Importe,=VtasProrrateo.Porcentaje,
VtasProrrateo.Porcentaje,=Sucursal.Sucursal,
Sucursal.Sucursal,=Sucursal.Nombre,
Sucursal.Nombre,=CentroCostos.CentroCostos,
CentroCostos.CentroCostos,=CentroCostos.Descripcion,
CentroCostos.Descripcion,=CentroCostos2.CentroCostos2,
CentroCostos2.CentroCostos2,=CentroCostos2.Descripcion,
CentroCostos2.Descripcion,=CentroCostos3.CentroCostos3,
CentroCostos3.CentroCostos3,=CentroCostos3.Descripcion,
CentroCostos3.Descripcion,=UEN.UEN,
UEN.UEN,=UEN.Nombre
UEN.Nombre=FROM VtasProrrateo
FROM VtasProrrateo=JOIN Sucursal ON VtasProrrateo.SucursalProrrateo<IGUAL>Sucursal.Sucursal
JOIN Sucursal ON VtasProrrateo.SucursalProrrateo<IGUAL>Sucursal.Sucursal=LEFT OUTER JOIN CentroCostos ON VtasProrrateo.CCProrrateo<IGUAL>CentroCostos.CentroCostos
LEFT OUTER JOIN CentroCostos ON VtasProrrateo.CCProrrateo<IGUAL>CentroCostos.CentroCostos=LEFT OUTER JOIN CentroCostos2 ON VtasProrrateo.CC2Prorrateo<IGUAL>CentroCostos2.CentroCostos2
LEFT OUTER JOIN CentroCostos2 ON VtasProrrateo.CC2Prorrateo<IGUAL>CentroCostos2.CentroCostos2=LEFT OUTER JOIN CentroCostos3 ON VtasProrrateo.CC3Prorrateo<IGUAL>CentroCostos3.CentroCostos3
LEFT OUTER JOIN CentroCostos3 ON VtasProrrateo.CC3Prorrateo<IGUAL>CentroCostos3.CentroCostos3=LEFT OUTER JOIN UEN ON VtasProrrateo.UENProrrateo<IGUAL>UEN.UEN
LEFT OUTER JOIN UEN ON VtasProrrateo.UENProrrateo<IGUAL>UEN.UEN=(Fin)

