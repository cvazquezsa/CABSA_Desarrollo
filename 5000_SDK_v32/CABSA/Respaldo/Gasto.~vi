CtaDinero.Tipo,=CtaDinero.Estatus,
CtaDinero.Estatus,=CtaDinero.Mensaje,
CtaDinero.Mensaje,=MensajeLista.Mensaje,
MensajeLista.Mensaje,=MensajeLista.Descripcion,
MensajeLista.Descripcion,=MensajeLista.Tipo,
MensajeLista.Tipo,=UEN.UEN,
UEN.UEN,=UEN.Nombre,
UEN.Nombre,=Art.Articulo,
Art.Articulo,=Art.Descripcion1,
Art.Descripcion1,=ActivoF.Serie,
ActivoF.Serie,=ProyectoDEnFirme.Asunto,
ProyectoDEnFirme.Asunto,=CentroCostos.CentroCostos,
CentroCostos.CentroCostos,=CentroCostos2.CentroCostos2,
CentroCostos2.CentroCostos2,=CentroCostos3.CentroCostos3,
CentroCostos3.CentroCostos3,=TipoComprobante.Concepto,
TipoComprobante.Concepto,=TipoRegistro.Concepto,
TipoRegistro.Concepto,=SustentoComprobante.Concepto,
SustentoComprobante.Concepto,=ContMoneda.ContMoneda,
ContMoneda.ContMoneda,=ContMoneda.TipoCambio
ContMoneda.TipoCambio=FROM Gasto
FROM Gasto=LEFT OUTER JOIN MovTipo ON {Comillas('GAS')}<IGUAL>MovTipo.Modulo AND Gasto.Mov<IGUAL>MovTipo.Mov
LEFT OUTER JOIN MovTipo ON {Comillas('GAS')}<IGUAL>MovTipo.Modulo AND Gasto.Mov<IGUAL>MovTipo.Mov=JOIN Mon ON Gasto.Moneda<IGUAL>Mon.Moneda
JOIN Mon ON Gasto.Moneda<IGUAL>Mon.Moneda=JOIN Prov ON Gasto.Acreedor<IGUAL>Prov.Proveedor
JOIN Prov ON Gasto.Acreedor<IGUAL>Prov.Proveedor=LEFT OUTER JOIN CtaDinero ON Gasto.CtaDinero<IGUAL>CtaDinero.CtaDinero
LEFT OUTER JOIN CtaDinero ON Gasto.CtaDinero<IGUAL>CtaDinero.CtaDinero=LEFT OUTER JOIN MensajeLista ON Gasto.Mensaje<IGUAL>MensajeLista.Mensaje
LEFT OUTER JOIN MensajeLista ON Gasto.Mensaje<IGUAL>MensajeLista.Mensaje=LEFT OUTER JOIN UEN ON Gasto.UEN<IGUAL>UEN.UEN
LEFT OUTER JOIN UEN ON Gasto.UEN<IGUAL>UEN.UEN=LEFT OUTER JOIN Art ON Gasto.AFArticulo<IGUAL>Art.Articulo
LEFT OUTER JOIN Art ON Gasto.AFArticulo<IGUAL>Art.Articulo=LEFT OUTER JOIN ActivoF ON Gasto.Empresa<IGUAL>ActivoF.Empresa AND Gasto.AFArticulo<IGUAL>ActivoF.Articulo AND Gasto.AFSerie<IGUAL>ActivoF.Serie
LEFT OUTER JOIN ActivoF ON Gasto.Empresa<IGUAL>ActivoF.Empresa AND Gasto.AFArticulo<IGUAL>ActivoF.Articulo AND Gasto.AFSerie<IGUAL>ActivoF.Serie=LEFT OUTER JOIN ProyectoDEnFirme ON Gasto.Proyecto<IGUAL>ProyectoDEnFirme.Proyecto AND Gasto.Actividad<IGUAL>ProyectoDEnFirme.Actividad
LEFT OUTER JOIN ProyectoDEnFirme ON Gasto.Proyecto<IGUAL>ProyectoDEnFirme.Proyecto AND Gasto.Actividad<IGUAL>ProyectoDEnFirme.Actividad=LEFT OUTER JOIN CentroCostos ON Gasto.ContUso<IGUAL>CentroCostos.CentroCostos
LEFT OUTER JOIN CentroCostos ON Gasto.ContUso<IGUAL>CentroCostos.CentroCostos=LEFT OUTER JOIN CentroCostos2 ON Gasto.ContUso2<IGUAL>CentroCostos2.CentroCostos2
LEFT OUTER JOIN CentroCostos2 ON Gasto.ContUso2<IGUAL>CentroCostos2.CentroCostos2=LEFT OUTER JOIN CentroCostos3 ON Gasto.ContUso3<IGUAL>CentroCostos3.CentroCostos3
LEFT OUTER JOIN CentroCostos3 ON Gasto.ContUso3<IGUAL>CentroCostos3.CentroCostos3=LEFT OUTER JOIN TipoComprobante ON Gasto.TipoComprobante<IGUAL>TipoComprobante.TipoComprobante
LEFT OUTER JOIN TipoComprobante ON Gasto.TipoComprobante<IGUAL>TipoComprobante.TipoComprobante=LEFT OUTER JOIN SustentoComprobante ON Gasto.SustentoComprobante<IGUAL>SustentoComprobante.SustentoComprobante
LEFT OUTER JOIN SustentoComprobante ON Gasto.SustentoComprobante<IGUAL>SustentoComprobante.SustentoComprobante=LEFT OUTER JOIN TipoRegistro ON Gasto.TipoIdentificacion<IGUAL>TipoRegistro.TipoRegistro
LEFT OUTER JOIN TipoRegistro ON Gasto.TipoIdentificacion<IGUAL>TipoRegistro.TipoRegistro=JOIN ContMoneda ON Gasto.Empresa<IGUAL>ContMoneda.Empresa
JOIN ContMoneda ON Gasto.Empresa<IGUAL>ContMoneda.Empresa=(Fin)

[Vista.ListaCalculados]
(Inicio)=ImporteTotal
ImporteTotal=Esp1
Esp1=Esp2
Esp2=Factor
Factor=ImporteF
ImporteF=ImpuestosF
ImpuestosF=RetencionF
RetencionF=SaldoF
SaldoF=ImporteTotalMC
ImporteTotalMC=ImporteFMN
ImporteFMN=ImpuestosFMN
ImpuestosFMN=RetencionFMN
RetencionFMN=SaldoFMN
SaldoFMN=(Fin)

[Vista.ListaRelaciones]
(Inicio)=Gasto.MovTipo
Gasto.MovTipo=Gasto.Mon
Gasto.Mon=Gasto.Prov
Gasto.Prov=Gasto.CtaDinero
Gasto.CtaDinero=Gasto.MensajeLista
Gasto.MensajeLista=Gasto.UEN
Gasto.UEN=Gasto.Art
Gasto.Art=Gasto.ActivoF
Gasto.ActivoF=Gasto.ProyectoDEnFirme
Gasto.ProyectoDEnFirme=Gasto.CentroCostos
Gasto.CentroCostos=Gasto.CentroCostos2
Gasto.CentroCostos2=Gasto.CentroCostos3
Gasto.CentroCostos3=Gasto.TipoComprobante
Gasto.TipoComprobante=Gasto.SustentoComprobante
Gasto.SustentoComprobante=Gasto.TipoRegistro
Gasto.TipoRegistro=Gasto.ContMoneda
Gasto.ContMoneda=Prov.CtaDinero
Prov.CtaDinero=Art.Prov
Art.Prov=Art.CentroCostos
Art.CentroCostos=Art.CentroCostos2
Art.CentroCostos2=Art.CentroCostos3
Art.CentroCostos3=ActivoF.Art
ActivoF.Art=ActivoF.CentroCostos
ActivoF.CentroCostos=ActivoF.UEN
ActivoF.UEN=ActivoF.ProyectoDEnFirme
ActivoF.ProyectoDEnFirme=(Fin)

[Vista.CamposBusquedaRapida]
(Inicio)=Gasto.Mov
Gasto.Mov=Gasto.MovID
Gasto.MovID=Gasto.Acreedor
Gasto.Acreedor=Gasto.Clase
Gasto.Clase=(Fin)
