[ImporteTotal]
Clave=ImporteTotal
TipoDatos=NumMonetario
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Importe Total
CondicionDespliege=MovTipo(<T>GAS<T>, Gasto:Gasto.Mov) en (GAS.DA, GAS.SR, GAS.ASC)
Monetario=S

Formula=Gasto.Importe-Gasto.Retencion+Gasto.Impuestos
