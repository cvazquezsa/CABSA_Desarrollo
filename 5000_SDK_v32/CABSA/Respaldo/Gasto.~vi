LlaveLocal=ActivoF.Proyecto<BR>ActivoF.Actividad
LlaveRemota=Proyecto<BR>Actividad
TipoRelacion=Opcional







[ImporteTotalMC]
Clave=ImporteTotalMC
TipoDatos=NumMonetario
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Importe Total (MN)

Monetario=S
CondicionDespliege=MovTipo(<T>GAS<T>, Gasto:Gasto.Mov) en (GAS.DA, GAS.SR, GAS.ASC)
Formula=((Gasto.Importe-Gasto.Retencion+Gasto.Impuestos)<BR>*Gasto.TipoCambio)/ContMoneda.TipoCambio
