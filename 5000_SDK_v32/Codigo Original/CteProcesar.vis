SQL=SELECT<BR>  Cte.Cliente,<BR>  Cte.Rama,<BR>  Cte.Nombre,<BR>  Cte.NombreCorto,<BR>  Cte.Numero,<BR>  Cte.Tipo,<BR>  Cte.Direccion,<BR>  Cte.DireccionNumero,<BR>  Cte.DireccionNumeroInt,<BR>  Cte.EntreCalles,<BR>  Cte.Plano,<BR>  Cte.Observaciones,<BR>  Cte.Delegacion,<BR>  Cte.Colonia,<BR>  Cte.CodigoPostal,<BR>  Cte.Poblacion,<BR>  Cte.Estado,<BR>  Cte.Pais,<BR>  Cte.Zona,<BR>  Cte.RFC,<BR>  Cte.Telefonos,<BR>  Cte.TelefonosLada,<BR>  Cte.Fax,<BR>  Cte.PedirTono,<BR>  Cte.Contacto1,<BR>  Cte.Contacto2,<BR>  Cte.eMail1,<BR>  Cte.eMail2,<BR>  Cte.DirInternet,<BR>  Cte.Categoria,<BR>  Cte.Familia,<BR>  Cte.Credito,<BR>  Cte.Grupo,<BR>  Cte.Clase,<BR>  Cte.Estatus,<BR>  Cte.Agente,<BR>  Cte.Ruta,<BR>  Cte.Situacion,<BR>  Cte.SituacionFecha,<BR>  Cte.SituacionUsuario,<BR>  Cte.SituacionNota,<BR>  Cte.CURP,<BR>  Cte.Espacio,<BR>  CtaSituacion.Icono,<BR>  ListaFiltro.Estacion,<BR>  ListaFiltro.ID,<BR>  ListaFiltro.Clave<BR><BR>FROM<BR>  Cte<BR>  LEFT OUTER JOIN CtaSituacion ON {Comillas(<T>CXC<T>)}=CtaSituacion.Rama AND Cte.Situacion=CtaSituacion.Situacion<BR>  JOIN ListaFiltro ON Cte.Cliente=ListaFiltro.Clave

