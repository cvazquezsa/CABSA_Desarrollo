Formula=Si(Config.CompraVerClaveIdioma, ArtIdioma.Codigo(hCompraD.Articulo, hCompra:hCompra.Idioma))

[SubImpuesto3]
Clave=SubImpuesto3
TipoDatos=NumMonetario
TipoCampo=Calculado
Monetario=S
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Impuesto 3

Formula=Si(Version.Impuesto3Info, 0, ImportePorcentaje(SubTotal+SubImpuesto2, hCompraD.Impuesto3))
[SubClaveFabricante]
Clave=SubClaveFabricante
Tamano=20
TipoCampo=Calculado
TamanoValidacion=20
Alineacion=por Omisión
Nombre=Clave Fabricante
Formula=Si(General.Autotransportes, SQL(<T>SELECT ClaveFabricante FROM ArtSub WHERE Articulo=:tArt AND SubCuenta=:tSub<T>, hCompraD.Articulo, hCompraD.SubCuenta))

[CantidadInvNeta]
Clave=CantidadInvNeta
TipoDatos=NumFlotante
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Inventario
Cantidad=S
CondicionDespliege=Config.MultiUnidades y Config.CompraFactorDinamico y (hCompra:hCompra.Estatus noen (EstatusSinAfectar, EstatusPorConfirmar))
Formula=(hCompraD.Cantidad-hCompraD.CantidadCancelada)*hCompraD.CantidadInventario/hCompraD.Cantidad

[Pendiente]
Clave=Pendiente
TipoDatos=NumMonetario
TipoCampo=Calculado
Monetario=S
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Pendiente
CondicionDespliege=(MovTipo(<T>COMS<T>, hCompra:hCompra.Mov)=COMS.PR) y (hCompra:hCompra.Estatus en (EstatusConcluido, EstatusPendiente))

Formula=Si((MovTipo(<T>COMS<T>, hCompra:hCompra.Mov)=COMS.PR) y (hCompra:hCompra.Estatus en (EstatusConcluido, EstatusPendiente)),<BR>SQL(<T>spVerPresupuestoCompraPendiente :tEmpresa, :fFecha, :fReq, :tArt, :tSub, :tMoneda, :tCC, :tCat, @ContUso2=:tCC2, @ContUso3=:tCC3<T>, Empresa, hCompra:hCompra.FechaEmision, hCompraD.FechaRequerida, hCompraD.Articulo, hCompraD.SubCuenta, hCompra:hCompra.Moneda, hCompraD.ContUso, hCompraD.Categoria, hCompraD.ContUso2, hCompraD.ContUso3))
