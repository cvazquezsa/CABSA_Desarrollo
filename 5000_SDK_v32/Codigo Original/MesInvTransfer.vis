[Vista.SQL]
(Inicio)=SELECT
SELECT=MesInvTransfer.Usuario,
MesInvTransfer.Usuario,=MesInvTransfer.ID,
MesInvTransfer.ID,=MesInvTransfer.Lanzamiento,
MesInvTransfer.Lanzamiento,=MesInvTransfer.Padre,
MesInvTransfer.Padre,=MesInvTransfer.Hijo,
MesInvTransfer.Hijo,=MesInvTransfer.Almacen,
MesInvTransfer.Almacen,=MesInvTransfer.AlmacenD,
MesInvTransfer.AlmacenD,=MesInvTransfer.Cantidad,
MesInvTransfer.Cantidad,=MesInvTransfer.CantidadA,
MesInvTransfer.CantidadA,=MesInvTransfer.UltimoPrecioCoste,
MesInvTransfer.UltimoPrecioCoste,=MesInvTransfer.PrecioCosteMedio,
MesInvTransfer.PrecioCosteMedio,=MesInvTransfer.UnidMedidaCompra,
MesInvTransfer.UnidMedidaCompra,=MesInvTransfer.UnidMedidaAlmacen,
MesInvTransfer.UnidMedidaAlmacen,=MesInvTransfer.TipoArticulo,
MesInvTransfer.TipoArticulo,=MesInvTransfer.Transferido,
MesInvTransfer.Transferido,=ArtDestino.Articulo,
ArtDestino.Articulo,=ArtDestino.Descripcion1,
ArtDestino.Descripcion1,=Art2.Articulo,
Art2.Articulo,=Art2.Descripcion1,
Art2.Descripcion1,=Art2.Tipo,
Art2.Tipo,=Art2.TipoOpcion,
Art2.TipoOpcion,=AlmDetalle.Almacen,
AlmDetalle.Almacen,=AlmDetalle.Nombre,
AlmDetalle.Nombre,=AlmDestino.Almacen,
AlmDestino.Almacen,=AlmDestino.Nombre,
AlmDestino.Nombre,=ArtDisponible.Empresa,
ArtDisponible.Empresa,=ArtDisponible.Articulo,
ArtDisponible.Articulo,=ArtDisponible.Almacen,
ArtDisponible.Almacen,=ArtDisponible.Disponible
ArtDisponible.Disponible=FROM MesInvTransfer
FROM MesInvTransfer=LEFT OUTER JOIN Art ArtDestino ON MesInvTransfer.Padre<IGUAL>ArtDestino.Articulo
LEFT OUTER JOIN Art ArtDestino ON MesInvTransfer.Padre<IGUAL>ArtDestino.Articulo=LEFT OUTER JOIN Art Art2 ON MesInvTransfer.Hijo<IGUAL>Art2.Articulo
LEFT OUTER JOIN Art Art2 ON MesInvTransfer.Hijo<IGUAL>Art2.Articulo=LEFT OUTER JOIN Alm AlmDetalle ON MesInvTransfer.Almacen<IGUAL>AlmDetalle.Almacen
LEFT OUTER JOIN Alm AlmDetalle ON MesInvTransfer.Almacen<IGUAL>AlmDetalle.Almacen=LEFT OUTER JOIN Alm AlmDestino ON MesInvTransfer.AlmacenD<IGUAL>AlmDestino.Almacen
LEFT OUTER JOIN Alm AlmDestino ON MesInvTransfer.AlmacenD<IGUAL>AlmDestino.Almacen=LEFT OUTER JOIN ArtDisponible ON {Comillas(Empresa)}<IGUAL>ArtDisponible.Empresa AND MesInvTransfer.Hijo<IGUAL>ArtDisponible.Articulo AND MesInvTransfer.Almacen<IGUAL>ArtDisponible.Almacen
LEFT OUTER JOIN ArtDisponible ON {Comillas(Empresa)}<IGUAL>ArtDisponible.Empresa AND MesInvTransfer.Hijo<IGUAL>ArtDisponible.Articulo AND MesInvTransfer.Almacen<IGUAL>ArtDisponible.Almacen=(Fin)

