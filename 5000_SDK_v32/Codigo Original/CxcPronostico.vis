TipoCampo=Calculado
Monetario=S
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Moratorios
FormatoValidacion=(Monetario)
Formula=Si((CxcInfo.DiasMoratorios>0) y (Config.CxcMoratoriosTasa>0), CxcInfo.Saldo*CxcInfo.DiasMoratorios*(Config.CxcMoratoriosTasa/100))

