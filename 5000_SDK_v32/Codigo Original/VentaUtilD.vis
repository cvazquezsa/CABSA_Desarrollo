[Utilidad]
Clave=Utilidad
TipoDatos=NumMonetario
TipoCampo=Calculado
Monetario=S
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Utilidad

Formula=ImporteNeto-CostoNeto
[UtilidadPor]
Clave=UtilidadPor
TipoDatos=NumFlotante
TipoCampo=Calculado
FormatoValidacion=#,.00
TamanoValidacion=10
Alineacion=por Omisión
Nombre=%
Formula=Si(ConDatos(CostoNeto) y (CostoNeto<>0.0), (ImporteNeto/CostoNeto)-1*100*FactorV)

[CantidadNeta]
Clave=CantidadNeta
TipoDatos=NumFlotante
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Cantidad
Formula=VentaUtilD.CantidadFactor*FactorC

[FactorV]
Clave=FactorV
TipoDatos=NumFlotante
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Factor
Formula=Si(VentaUtilD.MovClave en (VTAS.D, VTAS.DF, VTAS.B), -1.0, 1.0)

[ImporteNeto]
Clave=ImporteNeto
TipoDatos=NumMonetario
TipoCampo=Calculado
Monetario=S
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Importe Venta

Formula=DisminuyePorcentaje(ImporteBruto, VentaUtilD.DescuentoGlobal)*FactorV
[CostoNeto]
Clave=CostoNeto
TipoDatos=NumMonetario
TipoCampo=Calculado
Monetario=S
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Costo

Formula=VentaUtilD.CostoTotal*FactorC
[ComisionNeta]
Clave=ComisionNeta
TipoDatos=NumMonetario
TipoCampo=Calculado
Monetario=S
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Comisión

Formula=VentaUtilD.Comision*FactorV
[ImporteBruto]
Clave=ImporteBruto
TipoDatos=NumMonetario
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Importe
Cantidad=S
Formula=Si(Config.VentaPreciosImpuestoIncluido, QuitarPorcentaje(VentaUtilD.Importe, JuntarImpuestos(VentaUtilD.Impuesto1+Vacio(VentaUtilD.Impuesto3, Art.Impuesto3), VentaUtilD.Impuesto2)), VentaUtilD.Importe)

[Margen]
Clave=Margen
TipoDatos=NumFlotante
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=% Margen
FormatoValidacion=#,.00
Formula=Si(VentaUtilD.MovClave<>VTAS.B, (Utilidad/ImporteNeto)*100)

[FactorC]
Clave=FactorC
TipoDatos=NumFlotante
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=F
Formula=Caso VentaUtilD.MovClave<BR>  Es VTAS.D Entonces -1.0<BR>  Es VTAS.DF Entonces -1.0<BR>  Es VTAS.B Entonces 0.0<BR>Sino<BR>  1.0<BR>Fin


































































