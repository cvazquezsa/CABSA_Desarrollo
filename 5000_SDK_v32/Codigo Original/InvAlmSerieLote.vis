(Inicio)=SELECT Art.Articulo,
SELECT Art.Articulo,='Descripcion' <IGUAL> Art.Descripcion1,
'Descripcion' <IGUAL> Art.Descripcion1,=Art.PrecioLista,
Art.PrecioLista,=Art.Precio2,
