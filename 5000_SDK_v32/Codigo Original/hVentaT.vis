Clave=SubImpuesto1
TipoDatos=NumFlotante
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Impuesto 1

Formula=ImportePorcentaje(SubTotal+Si(Version.Impuesto2BaseImpuesto1, SubImpuesto2, 0), Imp1)
[FechaPoliza]
