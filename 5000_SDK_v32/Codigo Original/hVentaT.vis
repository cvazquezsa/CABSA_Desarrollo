Clave=PrecioNeto
TipoDatos=NumFlotante
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Precio
Monetario=S

Formula=Si(Config.VentaPreciosImpuestoIncluido, QuitarPorcentaje(hVentaD.Precio-Imp3, JuntarImpuestos(Imp1, Imp2)), hVentaD.Precio)
[SubImpuesto2]
Clave=SubImpuesto2
TipoDatos=NumFlotante
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Impuesto 2
Monetario=S

Formula=ImportePorcentaje(SubTotal, Imp2)
