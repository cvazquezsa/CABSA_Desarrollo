SQL=SELECT<BR>  FormaExtra.ID,<BR>  FormaExtra.Empresa,<BR>  FormaExtra.Mov,<BR>  FormaExtra.MovID,<BR>  FormaExtra.FechaEmision,<BR>  FormaExtra.UltimoCambio,<BR>  FormaExtra.Concepto,<BR>  FormaExtra.Proyecto,<BR>  FormaExtra.Usuario,<BR>  FormaExtra.Autorizacion,<BR>  FormaExtra.Referencia,<BR>  FormaExtra.DocFuente,<BR>  FormaExtra.Observaciones,<BR>  FormaExtra.Estatus,<BR>  FormaExtra.Situacion,<BR>  FormaExtra.SituacionFecha,<BR>  FormaExtra.SituacionUsuario,<BR>  FormaExtra.SituacionNota,<BR>  FormaExtra.OrigenTipo,<BR>  FormaExtra.Origen,<BR>  FormaExtra.OrigenID,<BR>  FormaExtra.Ejercicio,<BR>  FormaExtra.Periodo,<BR>  FormaExtra.FechaRegistro,<BR>  FormaExtra.FechaConclusion,<BR>  FormaExtra.FechaCancelacion,<BR>  FormaExtra.Sucursal,<BR>  FormaExtra.UEN,<BR>  FormaExtra.SucursalOrigen,<BR>  FormaExtra.SucursalDestino,<BR>  FormaExtra.FormaTipo,<BR>  FormaExtra.Aplica,<BR>  FormaExtra.AplicaClave,<BR>  FormaExtra.eMail,<BR>  Proy.Proyecto,<BR>  Proy.Descripcion,<BR>  UEN.UEN,<BR>  UEN.Nombre,<BR>  FormaTipoFormas.NombreCorto<BR><BR>FROM<BR>  FormaExtra<BR>  LEFT OUTER JOIN UEN ON FormaExtra.UEN=UEN.UEN<BR>  LEFT OUTER JOIN Proy ON FormaExtra.Proyecto=Proy.Proyecto<BR>  LEFT OUTER JOIN FormaTipoFormas ON FormaExtra.FormaTipo=FormaTipoFormas.FormaTipo

