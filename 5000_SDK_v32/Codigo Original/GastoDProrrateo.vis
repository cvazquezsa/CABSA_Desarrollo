[Vista.SQL]
(Inicio)=SELECT
SELECT=GastoDProrrateo.ID,
GastoDProrrateo.ID,=GastoDProrrateo.Renglon,
GastoDProrrateo.Renglon,=GastoDProrrateo.RenglonSub,
GastoDProrrateo.RenglonSub,=GastoDProrrateo.Concepto,
GastoDProrrateo.Concepto,=GastoDProrrateo.RenglonID,
