GastoDProrrateo.SucursalProrrateo,=GastoDProrrateo.ContUso,
GastoDProrrateo.ContUso,=GastoDProrrateo.Porcentaje,
GastoDProrrateo.Porcentaje,=GastoDProrrateo.Importe,
GastoDProrrateo.Importe,=GastoDProrrateo.Sucursal,
GastoDProrrateo.Sucursal,=GastoDProrrateo.SucursalOrigen,
GastoDProrrateo.SucursalOrigen,=GastoDProrrateo.Proyecto,
GastoDProrrateo.Proyecto,=GastoDProrrateo.Actividad,
GastoDProrrateo.Actividad,=GastoDProrrateo.Espacio,
GastoDProrrateo.Espacio,=GastoDProrrateo.UEN,
GastoDProrrateo.UEN,=GastoDProrrateo.VIN,
GastoDProrrateo.VIN,=GastoDProrrateo.Fecha,
GastoDProrrateo.Fecha,=Sucursal.Sucursal,
Sucursal.Sucursal,=Sucursal.Nombre,
Sucursal.Nombre,=Sucursal.Estatus,
Sucursal.Estatus,=CentroCostos.CentroCostos,
CentroCostos.CentroCostos,=Espacio.Espacio,
Espacio.Espacio,=Espacio.Nombre,
Espacio.Nombre,=VIN.VIN,
VIN.VIN,=VIN.Articulo,
VIN.Articulo,=ProyectoDEnFirme.Asunto
ProyectoDEnFirme.Asunto=FROM GastoDProrrateo
FROM GastoDProrrateo=LEFT OUTER JOIN Sucursal ON GastoDProrrateo.SucursalProrrateo<IGUAL>Sucursal.Sucursal
LEFT OUTER JOIN Sucursal ON GastoDProrrateo.SucursalProrrateo<IGUAL>Sucursal.Sucursal=LEFT OUTER JOIN CentroCostos ON GastoDProrrateo.ContUso<IGUAL>CentroCostos.CentroCostos
LEFT OUTER JOIN CentroCostos ON GastoDProrrateo.ContUso<IGUAL>CentroCostos.CentroCostos=LEFT OUTER JOIN Espacio ON GastoDProrrateo.Espacio<IGUAL>Espacio.Espacio
LEFT OUTER JOIN Espacio ON GastoDProrrateo.Espacio<IGUAL>Espacio.Espacio=LEFT OUTER JOIN VIN ON GastoDProrrateo.VIN<IGUAL>VIN.VIN
LEFT OUTER JOIN VIN ON GastoDProrrateo.VIN<IGUAL>VIN.VIN=LEFT OUTER JOIN ProyectoDEnFirme ON GastoDProrrateo.Proyecto<IGUAL>ProyectoDEnFirme.Proyecto AND GastoDProrrateo.Actividad<IGUAL>ProyectoDEnFirme.Actividad
LEFT OUTER JOIN ProyectoDEnFirme ON GastoDProrrateo.Proyecto<IGUAL>ProyectoDEnFirme.Proyecto AND GastoDProrrateo.Actividad<IGUAL>ProyectoDEnFirme.Actividad=(Fin)

