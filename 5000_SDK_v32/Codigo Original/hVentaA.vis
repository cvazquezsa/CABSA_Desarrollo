Tamano=20
TipoCampo=Calculado
TamanoValidacion=20
Alineacion=por Omisión
Nombre=Estatus Contable
Formula=Si(hVenta.GenerarPoliza, <T>Pendiente<T>, Si(Vacio(hVenta.ContID), <T>N/A<T>, Si(Vacio(hVenta.PolizaID), <T>Sin Afectar<T>, <T>Contabilizado<T>)))

