[ImporteTotal]
Clave=ImporteTotal
TipoDatos=NumMonetario
TipoCampo=Calculado
Monetario=S
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Importe Total
Formula=DisminuyePorcentaje(hVenta.Importe, hVenta.DescuentoGlobal)+hVenta.Impuestos

[TotalNeto]
Clave=TotalNeto
TipoDatos=NumMonetario
TipoCampo=Calculado
Monetario=S
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Total Neto
Formula=ImporteTotal-hVenta.AnticiposFacturados-hVenta.Retencion

[Dias]
Clave=Dias
TipoDatos=Numérico
TipoCampo=Calculado
TipoCalculo=De la misma vista
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Días
FormulaSQL=CASE WHEN hVenta.Estatus=<T>PENDIENTE<T> THEN DATEDIFF(day, hVenta.FechaEmision, GETDATE()) END

[Suritdo]
Clave=Suritdo
TipoDatos=NumFlotante
TipoCampo=Calculado
TipoCalculo=De la misma vista
FormatoValidacion=#.##
TamanoValidacion=10
Alineacion=por Omisión
Nombre=% Surtido
FormulaSQL=(1-(hVenta.Saldo / NULLIF(hVenta.Importe, 0.0)))*100

[Throughput]
Clave=Throughput
TipoDatos=NumFlotante
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Throughput
FormatoValidacion=#,
Formula=hVenta.Saldo*Dias

[EstatusContable]
