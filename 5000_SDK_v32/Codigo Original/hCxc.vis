<BR>  hCxc.FechaRevision,<BR>  hCxc.ContUso,<BR>  hCxc.TieneTasaEsp,<BR>  hCxc.TasaEsp,<BR>  hCxc.Codigo,<BR>  Mon.TipoCambio,<BR>  Mon.TipoCambioVenta,<BR>  Mon.TipoCambioCompra,<BR>  Cte.Nombre,<BR>  Cte.NombreCorto,<BR>  Cte.Numero,<BR>  Cte.Tipo,<BR>  Cte.Direccion,<BR>  Cte.EntreCalles,<BR>  Cte.Plano,<BR>  Cte.Observaciones,<BR>  Cte.Colonia,<BR>  Cte.CodigoPostal,<BR>  Cte.Poblacion,<BR>  Cte.Estado,<BR>  Cte.Pais,<BR>  Cte.Zona,<BR>  Cte.RFC,<BR>  Cte.Telefonos,<BR>  Cte.Fax,<BR>  Cte.Categoria,<BR>  Cte.Familia,<BR>  Cte.Credito,<BR>  Cte.Grupo,<BR>  Cte.ZonaImpuesto,<BR>  Cte.Estatus,<BR>  Cte.Proyecto,<BR>  Cte.Agente,<BR>  Cte.EnviarA,<BR>  Cte.Condicion,<BR>  Cte.DefMoneda,<BR>  Cte.Mensaje,<BR>  Cte.Cobrador,<BR>  Cte.DescuentoRecargos,<BR>  Cte.Agente3,<BR>  Cte.Agente4,<BR>  CteEnviarA.Clave,<BR>  CteEnviarA.Nombre,<BR>  CteEnviarA.Direccion,<BR>  CteEnviarA.ZonaImpuesto,<BR>  CxcSaldo.Saldo,<BR>  MonSaldo.TipoCambio,<BR>  MonSaldo.TipoCambioVenta,<BR>  MonSaldo.TipoCambioCompra,<BR>  LC.Descripcion,<BR>  LC.VigenciaDesde,<BR>  LC.VigenciaCondicion,<BR>  LC.VigenciaHasta,<BR>  LC.TieneTasaEsp,<BR>  LC.TasaEsp,<BR>  TipoAmortizacion.Descripcion,<BR>  TipoTasa.Descripcion,<BR>  CtaDinero.Tipo,<BR>  CtaDinero.Estatus,<BR>  CtaDinero.Mensaje,<BR>  CxcPendiente.ID,<BR>  CxcPendiente.Empresa,<BR>  CxcPendiente.Mov,<BR>  CxcPendiente.MovID,<BR>  CxcPendiente.Cliente,<BR>  CxcPendiente.Moneda,<BR>  CxcPendiente.FechaEmision,<BR>  CxcPendiente.Vencimiento,<BR>  CxcPendiente.Saldo,<BR>  CxcPendiente.Referencia,<BR>  CxcPendiente.Estatus,<BR>  MovTipo.Factor,<BR>  Cajero.Nombre,<BR>  Cajero.Estatus,<BR>  AgenteValida.Nombre,<BR>  AgenteValida.Estatus,<BR>  UEN.Nombre,<BR>  Personal.ApellidoPaterno,<BR>  Personal.ApellidoMaterno,<BR>  Personal.Nombre,<BR>  CentroCostos.Descripcion<BR><BR>FROM<BR>  hCxc<BR>  JOIN Mon ON hCxc.Moneda=Mon.Moneda<BR>  JOIN Mon MonSaldo ON hCxc.ClienteMoneda=MonSaldo.Moneda<BR>  LEFT OUTER JOIN UEN ON hCxc.UEN=UEN.UEN<BR>  JOIN Cte ON hCxc.Cliente=Cte.Cliente<BR>  LEFT OUTER JOIN CxcSaldo ON hCxc.Empresa=CxcSaldo.Empresa AND hCxc.ClienteMoneda=CxcSaldo.Moneda AND hCxc.Cliente=CxcSaldo.Cliente<BR>  LEFT OUTER JOIN CtaDinero ON hCxc.CtaDinero=CtaDinero.CtaDinero<BR>  LEFT OUTER JOIN CxcPendiente ON hCxc.Empresa=CxcPendiente.Empresa AND hCxc.Cliente=CxcPendiente.Cliente AND hCxc.MovAplica=CxcPendiente.Mov AND hCxc.MovAplicaID=CxcPendiente.MovID<BR>  LEFT OUTER JOIN CteEnviarA ON hCxc.Cliente=CteEnviarA.Cliente AND hCxc.ClienteEnviarA=CteEnviarA.ID<BR>  JOIN MovTipo ON <T>CXC<T>=MovTipo.Modulo AND hCxc.Mov=MovTipo.Mov<BR>  LEFT OUTER JOIN Agente Cajero ON hCxc.Cajero=Cajero.Agente<BR>  LEFT OUTER JOIN Agente AgenteValida ON hCxc.Agente=AgenteValida.Agente<BR>  LEFT OUTER JOIN Personal ON hCxc.PersonalCobrador=Personal.Personal<BR>  LEFT OUTER JOIN LC ON hCxc.LineaCredito=LC.LineaCredito<BR>  LEFT OUTER JOIN TipoAmortizacion ON hCxc.TipoAmortizacion=TipoAmortizacion.TipoAmortizacion<BR>  LEFT OUTER JOIN TipoTasa ON hCxc.TipoTasa=TipoTasa.TipoTasa<BR>  LEFT OUTER JOIN VIN ON hCxc.VIN=VIN.VIN<BR>  LEFT OUTER JOIN CentroCostos ON hCxc.ContUso=CentroCostos.CentroCostos

