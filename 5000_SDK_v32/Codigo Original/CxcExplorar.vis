JOIN ContMoneda ON CxcInfo.Empresa<IGUAL>ContMoneda.Empresa=JOIN Cxc ON CxcInfo.ID<IGUAL>Cxc.ID
JOIN Cxc ON CxcInfo.ID<IGUAL>Cxc.ID=(Fin)

