SQL=SELECT<BR>  aroRiesgo.Riesgo,<BR>  aroRiesgo.FechaRegistro,<BR>  aroRiesgo.Nombre,<BR>  aroRiesgo.Descripcion,<BR>  aroRiesgo.Comentarios,<BR>  aroRiesgo.Procedimiento,<BR>  aroRiesgo.Proceso,<BR>  aroRiesgo.CentroCostos,<BR>  aroRiesgo.UnidadOrganizacional,<BR>  aroRiesgo.LineaNegocios,<BR>  aroRiesgo.Supervisor,<BR>  aroRiesgo.Usuario,<BR>  aroRiesgo.Estatus,<BR>  aroRiesgo.UltimoCambio,<BR>  aroRiesgo.Alta,<BR>  aroRiesgo.Baja,<BR>  aroRiesgo.NivelAcceso,<BR>  aroRiesgo.Situacion,<BR>  aroRiesgo.SituacionFecha,<BR>  aroRiesgo.SituacionUsuario,<BR>  aroRiesgo.SituacionNota,<BR>  aroRiesgo.DetectadoPor,<BR>  aroRiesgo.DetectadoPorPuesto,<BR>  aroRiesgo.TieneMovimientos,<BR>  aroRiesgo.FechaAutorizacion,<BR>  aroRiesgo.FechaCancelacion,<BR>  aroRiesgo.UsuarioAutorizacion,<BR>  aroRiesgo.UsuarioCancelacion,<BR>  aroProcedimiento.Nombre,<BR>  aroProceso.Nombre,<BR>  aroUnidad.Nombre,<BR>  aroLinea.Nombre,<BR>  CentroCostos.Descripcion,<BR>  Recurso.Nombre<BR><BR>FROM<BR>  aroRiesgo<BR>  LEFT OUTER JOIN aroProcedimiento ON aroRiesgo.Procedimiento=aroProcedimiento.Procedimiento<BR>  LEFT OUTER JOIN aroProceso ON aroRiesgo.Proceso=aroProceso.Proceso<BR>  LEFT OUTER JOIN CentroCostos ON aroRiesgo.CentroCostos=CentroCostos.CentroCostos<BR>  LEFT OUTER JOIN aroUnidad ON aroRiesgo.UnidadOrganizacional=aroUnidad.UnidadOrganizacional<BR>  LEFT OUTER JOIN aroLinea ON aroRiesgo.LineaNegocios=aroLinea.LineaNegocios<BR>  LEFT OUTER JOIN Recurso ON aroRiesgo.Supervisor=Recurso.Recurso

