[PesoTotal]
Clave=PesoTotal
TipoDatos=NumFlotante
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Peso









CondicionDespliege=Config.VentaVerPesoVolumen
FormatoValidacion=#,.##
Formula=Si SQL(<T>SELECT VentaVerPesoVolumen FROM EmpresaCfg2 WHERE Empresa = :tEmp<T>,Empresa)<BR> Entonces<BR>  Si (SQL(<T>SELECT NivelFactorMultiUnidad FROM EmpresaCfg2 WHERE Empresa = :tEmp<T>,VentaPOS:VentaPOS.Empresa) = <T>Unidad<T>)<BR>   Entonces<BR>    (SQL(<T>SELECT Factor FROM Unidad WHERE Unidad = :tUnidad<T>,VentaDPOS.Unidad) * (VentaDPOS.Cantidad * Art.Peso))/SQL(<T>SELECT u.Factor FROM Art a JOIN Unidad u ON a.Unidad = u.Unidad WHERE a.Articulo = :tArt<T>,VentaDPOS.Articulo)<BR>   Sino<BR>    (SQL(<T>SELECT Factor FROM ArtUnidad WHERE Unidad = :tUnidad AND Articulo = :tArt<T>,VentaDPOS.Unidad,VentaDPOS.Articulo) * (VentaDPOS.Cantidad * Art.Peso))/SQL(<T>SELECT u.Factor FROM Art a JOIN ArtUnidad u ON a.Unidad = u.Unidad AND a.Articulo = u.Articulo WHERE a.Articulo = :tArt<T>,VentaDPOS.Articulo)<BR>  Fin<BR> Sino<BR>  VentaDPOS.Cantidad * Art.Peso<BR>Fin<BR>//VentaDPOS.Cantidad * Art.Peso
[VolumenTotal]
Clave=VolumenTotal
TipoDatos=NumFlotante
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Volumen

CondicionDespliege=Config.VentaVerPesoVolumen




FormatoValidacion=#,.##
























Formula=Si SQL(<T>SELECT VentaVerPesoVolumen FROM EmpresaCfg2 WHERE Empresa = :tEmp<T>,Empresa)<BR> Entonces<BR>  Si (SQL(<T>SELECT NivelFactorMultiUnidad FROM EmpresaCfg2 WHERE Empresa = :tEmp<T>,VentaPOS:VentaPOS.Empresa) = <T>Unidad<T>)<BR>   Entonces<BR>    (SQL(<T>SELECT Factor FROM Unidad WHERE Unidad = :tUnidad<T>,VentaDPOS.Unidad) * (VentaDPOS.Cantidad * Art.Volumen))/SQL(<T>SELECT u.Factor FROM Art a JOIN Unidad u ON a.Unidad = u.Unidad WHERE a.Articulo = :tArt<T>,VentaDPOS.Articulo)<BR>   Sino<BR>    (SQL(<T>SELECT Factor FROM ArtUnidad WHERE Unidad = :tUnidad AND Articulo = :tArt<T>,VentaDPOS.Unidad,VentaDPOS.Articulo) * (VentaDPOS.Cantidad * Art.Volumen))/SQL(<T>SELECT u.Factor FROM Art a JOIN ArtUnidad u ON a.Unidad = u.Unidad AND a.Articulo = u.Articulo WHERE a.Articulo = :tArt<T>,VentaDPOS.Articulo)<BR>  Fin<BR> Sino<BR>  VentaDPOS.Cantidad * Art.Peso<BR>Fin<BR>//VentaDPOS.Cantidad * Art.Volumen
