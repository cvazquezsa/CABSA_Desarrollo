MovTipo.SubClave=FROM TMA
FROM TMA=LEFT OUTER JOIN UEN ON TMA.UEN<IGUAL>UEN.UEN
LEFT OUTER JOIN UEN ON TMA.UEN<IGUAL>UEN.UEN=LEFT OUTER JOIN Proy ON TMA.Proyecto<IGUAL>Proy.Proyecto
LEFT OUTER JOIN Proy ON TMA.Proyecto<IGUAL>Proy.Proyecto=LEFT OUTER JOIN Alm ON TMA.Almacen<IGUAL>Alm.Almacen
LEFT OUTER JOIN Alm ON TMA.Almacen<IGUAL>Alm.Almacen=LEFT OUTER JOIN Agente ON TMA.Agente<IGUAL>Agente.Agente
LEFT OUTER JOIN Agente ON TMA.Agente<IGUAL>Agente.Agente=LEFT OUTER JOIN MovTipo ON {Comillas('TMA')}<IGUAL>MovTipo.Modulo AND TMA.Mov<IGUAL>MovTipo.Mov
LEFT OUTER JOIN MovTipo ON {Comillas('TMA')}<IGUAL>MovTipo.Modulo AND TMA.Mov<IGUAL>MovTipo.Mov=(Fin)

