TMA.Agente,=TMA.MontaCarga,
TMA.MontaCarga,=TMA.Prioridad,
TMA.Prioridad,=TMA.TarimaSurtido,
TMA.TarimaSurtido,=Proy.Proyecto,
Proy.Proyecto,=Proy.Descripcion,
Proy.Descripcion,=UEN.UEN,
UEN.UEN,=UEN.Nombre,
UEN.Nombre,=Alm.Nombre,
Alm.Nombre,=Alm.DefPosicionRecibo,
Alm.DefPosicionRecibo,=Agente.Nombre,
Agente.Nombre,=MovTipo.Clave,
