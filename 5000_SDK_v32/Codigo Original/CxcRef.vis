TipoDatos=NumMonetario
TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Al Corriente
Monetario=S
Formula=Si(Dias<=0, CxcRef.Saldo)

[Plazo1]
