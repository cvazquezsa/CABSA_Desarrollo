[Pronostico]
Clave=Pronostico
TipoDatos=Numérico
TipoCampo=Calculado
TipoCalculo=De la misma vista
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Pronóstico
FormulaSQL=CASE WHEN <BR>Saldo <= 0 OR DATEDIFF(day, GetDate(), Vencimiento)<0<BR>THEN<BR>-1<BR>WHEN DATEDIFF(day, GetDate(), Vencimiento) BETWEEN 0 AND 7 THEN DATEDIFF(day, GetDate(), Vencimiento) <BR>WHEN DATEDIFF(day, GetDate(), Vencimiento) BETWEEN 8 AND 14 THEN 14<BR>WHEN DATEDIFF(day, GetDate(), Vencimiento) BETWEEN 15 AND 30 THEN 30<BR>WHEN DATEDIFF(day, GetDate(), Vencimiento) BETWEEN 31 AND 60 THEN 60<BR>WHEN DATEDIFF(day, GetDate(), Vencimiento) BETWEEN 61 AND 90 THEN 90<BR>ELSE 90 END

[PronosticoNombre]
Clave=PronosticoNombre
Tamano=100
TipoCampo=Calculado
TamanoValidacion=100
Alineacion=por Omisión
Nombre=Pronóstico
Formula=Si<BR>  (Pronostico>0) y (Pronostico<8)<BR>Entonces<BR>  FechaEnTexto(Hoy+Pronostico, <T>ddddddd<T>)<BR>Sino<BR>  Caso Pronostico<BR>    Es -1 Entonces <T>Vencido<T><BR>    Es 0 Entonces <T>Hoy<T><BR>    Es 14 Entonces <T>de 8 a 14 días<T><BR>    Es 30 Entonces <T>de 15 a 30 días<T><BR>    Es 60 Entonces <T>de 30 a 60 días<T><BR>    Es 90 Entonces <T>de 60 a 90 días<T><BR>    Es 91 Entonces <T>mas 90 días<T><BR>  Fin<BR>Fin

