[Vista.SQL]
(Inicio)=SELECT
SELECT=SabanaFiltro.Estacion,
SabanaFiltro.Estacion,=SabanaFiltro.OperadorVenta,
SabanaFiltro.OperadorVenta,=SabanaFiltro.Venta,
SabanaFiltro.Venta,=SabanaFiltro.OperadorExistencia,
SabanaFiltro.OperadorExistencia,=SabanaFiltro.Existencia,
SabanaFiltro.Existencia,=SabanaFiltro.FechaD,
SabanaFiltro.FechaD,=SabanaFiltro.FechaA,
SabanaFiltro.FechaA,=SabanaFiltro.SucursalGrupo,
SabanaFiltro.SucursalGrupo,=SabanaFiltro.Proveedor,
SabanaFiltro.Proveedor,=SabanaFiltro.Departamento,
SabanaFiltro.Departamento,=SabanaFiltro.Seccion,
SabanaFiltro.Seccion,=SabanaFiltro.Articulo,
SabanaFiltro.Articulo,=SabanaFiltro.Fabricante,
SabanaFiltro.Fabricante,=SabanaFiltro.MostrarProvOmision,
SabanaFiltro.MostrarProvOmision,=Prov.Nombre,
Prov.Nombre,=ArtCat.Categoria,
ArtCat.Categoria,=ArtFam.Familia,
ArtFam.Familia,=Art.Descripcion1,
Art.Descripcion1,=SucursalCat.Categoria,
SucursalCat.Categoria,=Fabricante.Fabricante
Fabricante.Fabricante=FROM SabanaFiltro
FROM SabanaFiltro=LEFT OUTER JOIN SucursalCat ON SabanaFiltro.SucursalGrupo<IGUAL>SucursalCat.Categoria
LEFT OUTER JOIN SucursalCat ON SabanaFiltro.SucursalGrupo<IGUAL>SucursalCat.Categoria=LEFT OUTER JOIN Prov ON SabanaFiltro.Proveedor<IGUAL>Prov.Proveedor
LEFT OUTER JOIN Prov ON SabanaFiltro.Proveedor<IGUAL>Prov.Proveedor=LEFT OUTER JOIN ArtCat ON SabanaFiltro.Departamento<IGUAL>ArtCat.Categoria
LEFT OUTER JOIN ArtCat ON SabanaFiltro.Departamento<IGUAL>ArtCat.Categoria=LEFT OUTER JOIN ArtFam ON SabanaFiltro.Seccion<IGUAL>ArtFam.Familia
LEFT OUTER JOIN ArtFam ON SabanaFiltro.Seccion<IGUAL>ArtFam.Familia=LEFT OUTER JOIN Art ON SabanaFiltro.Articulo<IGUAL>Art.Articulo
LEFT OUTER JOIN Art ON SabanaFiltro.Articulo<IGUAL>Art.Articulo=LEFT OUTER JOIN Fabricante ON SabanaFiltro.Fabricante<IGUAL>Fabricante.Fabricante
LEFT OUTER JOIN Fabricante ON SabanaFiltro.Fabricante<IGUAL>Fabricante.Fabricante=(Fin)

