TipoCampo=Calculado
TamanoValidacion=10
Alineacion=por Omisión
Nombre=Desc.
Formula=DescuentoEnCascada(DescLinea, CompraPendienteD.DescuentoGlobal)

[Asignado]
Clave=Asignado
Tamano=30
TipoCampo=Calculado
TamanoValidacion=30
Alineacion=por Omisión
Nombre=Asignado
